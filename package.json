{
  "name": "locals-scrapper-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build --emptyOutDir",
    "preview": "vite preview",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "format": "prettier --write .",
    "cdk:synth": "cdk synth",
    "cdk:bootstrap": "cdk bootstrap",
    "cdk:deploy:dev": "npm run build && cdk deploy LocalsScrapperAppDevStack",
    "cdk:deploy:prod": "npm run build && cdk deploy LocalsScrapperAppProdStack",
    "cdk:destroy": "cdk destroy --all",
    "openapi:generate": "openapi-generator-cli generate -i http://localhost:3000/api/swagger.json -g typescript-fetch -o ./src/api"
  },
  "dependencies": {
    "@emotion/cache": "^11.14.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^6.3.0",
    "@mui/material": "^6.3.0",
    "@mui/material-nextjs": "^6.3.0",
    "@mui/x-data-grid": "^7.23.5",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.80.0",
    "@tanstack/react-query": "^5.62.11",
    "@tanstack/react-query-devtools": "^5.62.11",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.1",
    "react-router-dom": "^7.1.1"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@openapitools/openapi-generator-cli": "^2.15.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.19.1",
    "@vitejs/plugin-react": "^4.3.4",
    "aws-cdk-lib": "2.173.2",
    "dotenv": "^16.4.7",
    "envalid": "^8.1.1",
    "eslint": "^9",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "globals": "^15.14.0",
    "prettier": "^3.4.2",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vite": "^5.0.9",
    "vitest": "^1.1.3"
  },
  "overrides": {
    "@emotion/serialize": "1.2.0"
  }
}
